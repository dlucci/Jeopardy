<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".JeopardyActivity"
    android:id="@+id/gridLayout"
    android:rowCount="11"
    android:columnCount="2"
    android:background=""
    >
    <TextView
        android:id="@+id/score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:layout_column="0"
        android:layout_row="0"
        />
    <Button
        android:id="@+id/plus200"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="200"
        android:layout_column="0"
        android:layout_row="1"
        />
    <Button
        android:id="@+id/minus200"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="-200"
        android:layout_column="1"
        android:layout_row="1"
        />
    <Button
        android:id="@+id/plus400"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="400"
        android:layout_column="0"
        android:layout_row="2"/>
    <Button
        android:id="@+id/minus400"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="-400"
        android:layout_column="1"
        android:layout_row="2"/>
    <Button
        android:id="@+id/plus600"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="600"
        android:layout_column="0"
        android:layout_row="3"/>
    <Button
        android:id="@+id/minus600"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="-600"
        android:layout_column="1"
        android:layout_row="3"/>
    <Button
        android:id="@+id/plus800"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="800"
        android:layout_column="0"
        android:layout_row="4"/>
    <Button
        android:id="@+id/minus800"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="-800"
        android:layout_column="1"
        android:layout_row="4"/>
    <Button
        android:id="@+id/plus1000"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="1000"
        android:layout_column="0"
        android:layout_row="5"
        />
    <Button
        android:id="@+id/minus1000"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="-1000"
        android:layout_column="1"
        android:layout_row="5"/>
    <Button
        android:id="@+id/plus1200"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="1200"
        android:layout_column="0"
        android:layout_row="6"
        />
    <Button
        android:id="@+id/minus1200"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="-1200"
        android:layout_column="1"
        android:layout_row="6"
        />
    <Button
        android:id="@+id/plus1600"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="1600"
        android:layout_column="0"
        android:layout_row="7"
        />
    <Button
        android:id="@+id/minus1600"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="-1600"
        android:layout_column="1"
        android:layout_row="7"/>
    <Button
        android:id="@+id/plus2000"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="2000"
        android:layout_column="0"
        android:layout_row="8"
        />
    <Button
        android:id="@+id/minus2000"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="-2000"
        android:layout_column="1"
        android:layout_row="8"/>

</GridLayout>
